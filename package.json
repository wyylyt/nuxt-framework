{
  "name": "nuxt-framework",
  "version": "1.0.0",
  "description": "nuxt基础脚手架支持docker容器化",
  "author": "wanjikun",
  "private": true,
  "scripts": {
    "dev": "cross-env MODE=DEV NODE_ENV=development NODE_MODE=DEV nodemon server/index.js --watch server",
    "build": "cross-env MODE=PRO nuxt build",
    "build:release": "cross-env MODE=PRO nuxt build",
    "build:pre-release": "cross-env MODE=PRE nuxt build",
    "build:testing": "cross-env MODE=TEST nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "docker": "docker build -t cqliving-fd-pc .",
    "docker:run": "docker run -dt --name cqliving-fd-pc -p 20001:20001 cqliving-fd-pc"
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@types/ali-oss": "^6.0.5",
    "@types/js-md5": "^0.4.2",
    "ali-oss": "^6.8.0",
    "cross-env": "^5.2.0",
    "iview": "^3.1.5",
    "js-md5": "^0.7.3",
    "koa": "^2.6.2",
    "lodash": "^4.17.21",
    "moment": "^2.25.3",
    "nuxt": "^2.0.0",
    "v-viewer": "^1.5.1",
    "vue-baidu-map": "^0.21.22",
    "vue-lazyload": "^1.3.3",
    "vue-property-decorator": "8.4.1",
    "sass": "^1.26.3"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.6.0",
    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.0.0",
    "lint-staged": "^10.0.0",
    "node-sass": "^4.14.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2"
  }
}
